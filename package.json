{
  "name": "CozySound",
  "version": "0.0.1",
  "engines": [
    "node = 0.10.x"
  ],
  "description": "A sound app for Cozy",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Peltoche/CozySound"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.11",
    "axon": "0.6.1",
    "cozydb": "0.0.19",
    "moment": "2.6.0",
    "multiparty": "3.3.0",
    "node-polyglot": "0.3.0",
    "cozy-i18n-helper": "0.0.4"
  },
  "devDependencies": {
    "should": "4.0.1",
    "mocha": "1.20.1",
    "request-json": "0.4.10",
    "cozy-fixtures": "1.1.1",
    "coffeelint": "1.5.1",
    "printit": "^0.1.3"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node build/server.js"
  },
  "cozy-permissions": {
      "Track": {
                "description": "Store tracks in your cozy"
      },
      "Playlist": {
                "description": "Store playlists in your cozy"
      },
      "Album": {
                "description": "Store albums in your cozy"
      }
  },
  "cozy-displayName": "Sound"
}
